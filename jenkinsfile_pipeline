addressbook/Jenkinsfile_CI

root Jenkins CI
 History
 0 contributors
101 lines (43 sloc)  980 Bytes
pipeline {

    agent{

            label 'master'

    }

    tools {



           maven 'maven' 

           jdk 'java8'

    }



    stages{

        stage ('Code Validate'){

            steps {

                

                sh """

                echo "Perform Validate"

                mvn validate

                """



            }

        }

        stage ('Code Compilation'){

            steps {

                sh """

                echo "Perform Compilation"

                mvn compile

                """

            }

        }



        stage ('Code Test'){

            steps{

                sh """

                echo "To test the code"

                mvn test

                """

            }

        }

        stage ('Code Package') {

            steps{

                sh """

                echo "Create the package"

                mvn package

                """

            }

        }



        }

    

    }


